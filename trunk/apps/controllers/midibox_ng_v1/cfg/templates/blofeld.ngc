
################################################################################
# Reset the setup to default settings
# This command ensures, that previous hardware definitions will be overwritten
# by a new setup without the need to set defintions for all components
################################################################################
RESET_HW

LCD "%C@(1:1:1)Blofeld Ctrl"


################################################################################
# We provide 5 banks which are only used for encoders
################################################################################
# Start ID/End ID/First HW Encoder
BANK n=1  enc=1:16:1
BANK n=2  enc=1:16:17
BANK n=3  enc=1:16:33
BANK n=4  enc=1:16:49
BANK n=5  enc=1:16:65


################################################################################
# Following buttons switch between the Encoder groups
# For demonstration purposes, we also add a second meta event which switches the AINSER group (but AINSER not configured here...)
# The selected group will be displayed at lcd_pos=1:1:1 (first LCD, X=1, Y=1)
################################################################################
EVENT_BUTTON id=  1  type=Meta   meta=SetBank button_mode=OnOnly range=1:1  lcd_pos=1:1:1 label="**** OSC1 Bank  ****"
EVENT_BUTTON id=  2  type=Meta   meta=SetBank button_mode=OnOnly range=2:2  lcd_pos=1:1:1 label="**** OSC2 Bank  ****"
EVENT_BUTTON id=  3  type=Meta   meta=SetBank button_mode=OnOnly range=3:3  lcd_pos=1:1:1 label="**** OSC3 Bank  ****"
EVENT_BUTTON id=  4  type=Meta   meta=SetBank button_mode=OnOnly range=4:4  lcd_pos=1:1:1 label="**** FIL1 Bank  ****"
EVENT_BUTTON id=  5  type=Meta   meta=SetBank button_mode=OnOnly range=5:5  lcd_pos=1:1:1 label="**** FIL2 Bank  ****"


################################################################################
# this button requests a SysEx dump
# note: instead of "^bnk ^pat" we specify "0x7f 0x00" to request the edit buffer!
################################################################################
EVENT_BUTTON id=  8  type=SysEx  stream="0xf0 0x3e 0x13 ^dev 0x00 0x7f 0x00 0xf7" \
             range=  0:1 offset=  0  ports=1000100000001000  button_mode=OnOnly \
             lcd_pos=1:1:1 label="*** Dump Request ***"

# this generic receiver (without controller assignment) receives the SysEx dump
# and notifies all events which select syxdump_pos=1:<pos-in-dump>
EVENT_RECEIVER id=  1  type=SysEx  stream="0xf0 0x3e 0x13 ^dev 0x10 ^ignore ^ignore ^dump" \
             ports=1000100000001000 \
             lcd_pos=1:1:1 label="* Received SyxDump *"


