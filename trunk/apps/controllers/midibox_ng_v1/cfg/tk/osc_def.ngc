# My preferred ethernet setup

ETH_LocalIp 192.168.1.180
ETH_Netmask 255.255.255.0
ETH_Gateway 192.168.1.1
ETH_Dhcp 1

# iPad with MCMPP protocol
OSC_RemoteIp 0 192.168.1.110
OSC_RemotePort 0 8001
