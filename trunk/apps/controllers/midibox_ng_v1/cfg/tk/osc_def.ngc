OSC_TransferMode 1 4

# Mac with TouchOSC protocol
OSC_RemoteIp 2 192.168.1.114
OSC_RemotePort 2 8001
OSC_LocalPort 2 8000
OSC_TransferMode 2 4

# Mac with MIDI protocol
OSC_RemoteIp 3 192.168.1.114
