# tests banks on my MBLC hardware

RESET_HW
LCD "%C@(1:1:1)LC Banks Test"

# encoder 1..8 are assigned to ID 21..28, 29..36, 37..44, 45..52
BANK n=1  enc=1:8:21  button=1:8:21
BANK n=2  enc=1:8:29  button=1:8:29
BANK n=3  enc=1:8:37  button=1:8:37
BANK n=4  enc=1:8:45  button=1:8:45

# select the bank with the jogwheel
EVENT_ENC id=  9  type=Meta   meta=SetBank range=1:4  lcd_pos=1:1:2 label="Bank #%q"

# use button 9 as "FAST" button (it's outside the bank)
EVENT_BUTTON id=9 type=Meta   meta=EncFast  range=0:5 lcd_pos=1:10:2 label="FAST%d"


